{
  "name": "WikiLinks",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Inserts links to sections on Wikipedia.",
  "icons": {
    "128": "wikilinks128.png",
    "48": "wikilinks48.png",
    "16": "wikilinks16.png"
  },
  "permissions": [
    "http://*.wikipedia.org/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.wikipedia.org/*"],
      "js": ["wikilinks.js"]
    }
  ]
}
